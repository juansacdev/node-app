<div class="row">
    {{#each notes}}
        <div class="col-md-3">
            <div class="card m-3">
                <div class="card-body m-3">
                    <h4 class="card-title">
                        {{ title }}
                    </h4>
                    <p>
                        {{ description }}
                    </p>
                </div>
            </div>
        </div>
    {{/each}}
</div>